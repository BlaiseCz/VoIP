<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="mainPane" xmlns="http://javafx.com/javafx/8"
            fx:controller="gui.components.LoginController">
    <children>
        <TextField fx:id="usernameTF" layoutX="220.0" layoutY="118.0" promptText="Username"
                   AnchorPane.bottomAnchor="200.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="200.0"/>
        <PasswordField fx:id="passwordTF" layoutX="220.0" layoutY="223.0" promptText="Password"
                       AnchorPane.bottomAnchor="150.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="200.0"/>
        <Button fx:id="loginBtn" layoutX="262.0" layoutY="305.0" mnemonicParsing="false" text="Login"
                AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="200.0" AnchorPane.rightAnchor="200.0"/>
    </children>
    <stylesheets>
        <URL value="@login.css"/>
    </stylesheets>
</AnchorPane>
